<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="health.model.HealthDate">
	<select id="GetMyHealthDateList" resultType="health.model.HealthDateBean">
		select * from healthDate where id=#{id}
		order by hdate desc
	</select>
	
	<select id="GetHealthByHdate" resultType="health.model.HealthDateBean">
		select * from healthDate where hdate=#{hdate}
	</select>
	
	<select id="InsertHealthDate">
		insert into healthDate values(hseq.nextval, #{id}, #{hdate})
	</select>
</mapper>