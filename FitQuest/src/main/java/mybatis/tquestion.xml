<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="question.model.Tquestion">
	<insert id="InsertTQuestion">
		INSERT INTO TQUESTION 
		VALUES (
			    tqseq.nextval
			   ,#{id}
			   ,#{tqemail}
			   ,sysdate
			   ,#{tqsubject}
			   ,#{tqcontent}
			   ,#{tqanswer}
			   )
	</insert>
</mapper>